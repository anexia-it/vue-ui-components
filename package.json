{
  "name": "ui",
  "version": "0.1.13",
  "description": "Anexia Mini Tool UI",
  "private": true,
  "scripts": {
    "serve": "npm run generate:icons && vue-cli-service serve",
    "build": "npm run generate:icons  && npm run build:bundle && npm run build:app",
    "build:app": "npm run generate:icons && vue-cli-service build",
    "build:bundle": "vue-cli-service build --target lib --inline-vue --dest lib ./src/index.ts",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lint:icons": "vue-cli-service lint ./src/icons/*.js ./src/icons/*.ts",
    "generate:icons": "node ./src/icons/helpers/generate-files.js && npm run lint:icons"
  },
  "main": "lib/ui.umd.js",
  "module": "lib/ui.common.js",
  "unpkg": "lib/ui.umd.min.js",
  "style": "lib/ui.css",
  "author": "ANEXIA",
  "files": [
    "lib/*",
    "nuxt/*",
    "index.d.ts"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@types/webpack": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "bootstrap": "^4.5.0",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-functional-data-merge": "^3.1.0",
    "vue-i18n": "^8.21.0",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11"
  }
}
