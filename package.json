{
  "name": "@anx/vue-ui-components",
  "version": "1.18.1",
  "description": "A Vue component library containing several components that match the ANEXIA style guidelines",
  "author": "ANEXIA <marketing@anexia-it.com> (https://anexia.com)",
  "license": "MIT",
  "scripts": {
    "serve": "npm run generate:icons && vue-cli-service serve",
    "build": "npm run build:bundle && npm run build:app",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:app": "npm run generate:icons && vue-cli-service build",
    "build:bundle": "npm run generate:icons && vue-cli-service build --target lib --inline-vue --dest lib ./src/index.ts",
    "build:lib": "rm -rf lib && vue-cli-service build --target lib --dest lib --name anx ./src/index.ts",
    "generate:icons": "node ./src/plugins/icons/helpers/generate-files.js && npm run lint:icons",
    "git:add-bundle": "ts-node git/pre-commit.ts",
    "lint:icons": "vue-cli-service lint ./src/plugins/icons/*.js ./src/plugins/icons/*.ts",
    "styleguide": "vue-styleguidist server",
    "build:docs": "vue-styleguidist build"
  },
  "main": "lib/anx.common.js",
  "module": "lib/anx.umd.js",
  "style": "lib/anx.css",
  "files": [
    "lib/*",
    "nuxt/*",
    "src/*",
    "index.d.ts"
  ],
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/chai": "^4.2.11",
    "@types/jest": "^24.0.19",
    "@types/webpack": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "git-branch-is": "^4.0.0",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.2.0",
    "jsdom-global": "^3.0.2",
    "node-html-parser": "^2.0.2",
    "null-loader": "^4.0.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "simple-git": "^2.20.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.10",
    "url-parse": "^1.4.7",
    "vue": "^2.6.11",
    "vee-validate": "^2.2.15",
    "vue-class-component": "^7.2.3",
    "vue-functional-data-merge": "^3.1.0",
    "vue-i18n": "^8.21.0",
    "vue-jest": "^3.0.7",
    "vue-property-decorator": "^8.4.2",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.5.2",
    "vue-styleguidist": "^4.39.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "if git-branch-is integration; then npm run build:bundle && npm run git:add-bundle; fi"
    }
  }
}
